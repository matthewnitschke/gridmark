{
    "name": "Gridmark",
    "version": "1.0.0",
    "icons": { 
      "16": "icons/16.png",
      "48": "icons/48.png",
     "128": "icons/128.png" 
    },
    "manifest_version": 2,
    "options_page": "src/options/index.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_title": "Query Inject",
      "default_popup": "src/options/index.html"
    },
    "permissions": ["storage"]
  }
  