{
    "name": "Gridmark",
    "version": "1.0.0",
    "manifest_version": 2,
    "options_page": "dist/index.html",
    "browser_action": {
      "default_title": "Gridmark",
      "default_popup": "dist/index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": ["storage", "chrome://favicon/"]
  }
  